# ================================================
# QUANTUMSENTINEL PLATFORM SETTINGS
# ================================================

# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=quantumsentinel
POSTGRES_USER=quantum_user
POSTGRES_PASSWORD=your_secure_postgres_password_here

# Redis Configuration
REDIS_URL=redis://localhost:6379/0

# JWT Secret for authentication
JWT_SECRET=your_very_secure_jwt_secret_here_at_least_32_characters_long

# Encryption key for API key storage
ENCRYPTION_KEY=your_32_character_encryption_key_here

# Admin credentials
ADMIN_EMAIL=admin@quantumsentinel.local
ADMIN_PASSWORD=your_secure_admin_password_here

# Platform configuration
PLATFORM_NAME=QuantumSentinel-Nexus
ENVIRONMENT=production
DEBUG_MODE=false
LOG_LEVEL=INFO

# Email configuration (for notifications)
SMTP_HOST=smtp.your-email-provider.com
SMTP_PORT=587
SMTP_USERNAME=your_smtp_username_here
SMTP_PASSWORD=your_smtp_password_here
FROM_EMAIL=noreply@quantumsentinel.local

# Webhook URLs for integrations
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR/DISCORD/WEBHOOK
TEAMS_WEBHOOK_URL=https://your-org.webhook.office.com/webhookb2/YOUR/TEAMS/WEBHOOK

# Rate limiting configuration
RATE_LIMIT_PER_MINUTE=100
RATE_LIMIT_BURST=20

# File upload limits
MAX_UPLOAD_SIZE_MB=100
ALLOWED_FILE_TYPES=pdf,doc,docx,txt,log,pcap,exe,dll,bin

# Session configuration
SESSION_TIMEOUT_HOURS=24
MAX_CONCURRENT_SCANS=10
MAX_SCAN_DURATION_HOURS=24