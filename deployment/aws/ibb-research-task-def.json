{
    "family": "quantumsentinel-ibb-research",
    "taskRoleArn": "arn:aws:iam::077732578302:role/quantumsentinel-nexus-execution-role",
    "executionRoleArn": "arn:aws:iam::077732578302:role/quantumsentinel-nexus-execution-role",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048",
    "containerDefinitions": [
        {
            "name": "ibb-research",
            "image": "077732578302.dkr.ecr.us-east-1.amazonaws.com/quantumsentinel-ibb-research:comprehensive",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8002,
                    "hostPort": 8002,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "SERVICE_NAME",
                    "value": "IBB_RESEARCH"
                },
                {
                    "name": "SERVICE_PORT",
                    "value": "8002"
                },
                {
                    "name": "AWS_S3_REPORTS_BUCKET",
                    "value": "quantum-sentinel-reports-1759046142"
                },
                {
                    "name": "AWS_DEFAULT_REGION",
                    "value": "us-east-1"
                }
            ],
            "secrets": [
                {
                    "name": "CHAOS_API_KEY",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:077732578302:secret:quantum/chaos-api-key"
                },
                {
                    "name": "CVE_API_KEY",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:077732578302:secret:quantum/cve-api-key"
                },
                {
                    "name": "NUCLEI_API_KEY",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:077732578302:secret:quantum/nuclei-api-key"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/quantumsentinel-nexus",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ibb-research"
                }
            }
        }
    ]
}